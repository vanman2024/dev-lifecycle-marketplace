# MCP Server API Keys
# Copy this file to .env in your project root and fill in your actual values

# Context7 - Up-to-date library documentation
CONTEXT7_API_KEY=ctx7sk-your-key-here

# GitHub - Repository access and Copilot API
GITHUB_TOKEN=ghp_your-token-here

# Figma - Design file access
FIGMA_ACCESS_TOKEN=figd_your-token-here

# Supabase - Database access
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key-here
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_PROJECT_REF=your-project-ref

# Postman - API collections and testing
POSTMAN_API_KEY=PMAK-your-key-here

# Airtable - Database access
AIRTABLE_TOKEN=patyour-token-here

# Browserbase - Browser automation
BROWSERBASE_API_KEY=your-key-here
BROWSERBASE_PROJECT_ID=your-project-id

# Slack - Team communication
SLACK_BOT_TOKEN=xoxb-your-token-here
SLACK_TEAM_ID=T0your-team-id

# Notion - Workspace access
NOTION_API_KEY=secret_your-key-here

# OpenMemory (mem0) - AI memory management
MEM0_API_KEY=your-mem0-key-here

# Google Cloud - Imagen/Veo content generation
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json
GOOGLE_API_KEY=your-google-api-key

# Anthropic - Claude API
ANTHROPIC_API_KEY=sk-ant-your-key-here

# Qwen - Alibaba LLM (if using Qwen MCP server)
QWEN_API_KEY=your-qwen-key-here

# Gemini - Google's LLM (if using Gemini MCP server)
GEMINI_API_KEY=your-gemini-key-here

# Codex - OpenAI Codex (if using Codex MCP server)
CODEX_API_KEY=your-codex-key-here
OPENAI_API_KEY=sk-your-openai-key-here
